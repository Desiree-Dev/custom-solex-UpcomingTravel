{
	"_version": "1.15.0",
	"sap.app": {
		"id": "custom.solex.UpcomingTravel",
		"type": "card",
		"title": "Upcoming Travel",
		"subTitle": "",
		"applicationVersion": {
			"version": "1.0.1"
		}
	},
	"sap.ui": {
		"technology": "UI5",
        "deviceTypes": {"desktop": true, "phone": true, "tablet": true},
		"icons": {
			"icon": "sap-icon://bus-public-transport"
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "custom.solex.UpcomingTravel.View",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.38",
			"libs": {
				"sap.m": {}
			}
		},
		"config": {
			"sample": {
				"stretch": true,
				"files": [
					"Card.controller.js"
				]
			}
		}
	},
	"sap.card": {
		"type": "Component",
        "designtime": "dt/configuration",
		"header": {
			
			"title": "Upcoming Travels",
            "subTitle": "Copenhagen",
            "actions": [
                {
                    "type": "Navigation",
                    "enabled": true,
                    "url": "https://www.concursolutions.com/home.asp"
                }
                ]
		}
	},
    "sap.platform.mobilecards": {
        "compatible": false
    }
}